{
  "name": "@outburn/fhir-client",
  "version": "1.3.0",
  "description": "A modern, lightweight FHIR client for TypeScript/JavaScript with support for R3, R4, and R5.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:all": "npm test && npm run test:integration",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "fhir",
    "client",
    "typescript",
    "health",
    "hl7"
  ],
  "author": "Outburn Ltd.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Outburn-IL/fhir-client.git"
  },
  "bugs": "https://github.com/Outburn-IL/fhir-client/issues",
  "homepage": "https://github.com/Outburn-IL/fhir-client#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "lru-cache": "^11.2.4"
  },
  "devDependencies": {
    "@outburn/types": "^0.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "docker-compose": "^1.3.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}
